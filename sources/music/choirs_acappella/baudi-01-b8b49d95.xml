    <music>
        <facsimile/>
        <body>
            <mdiv n="1" label="Ständchen">
                <score>
                    <scoreDef/>
                    <section/>
                    <div>
                        <lg n="1" type="stanza">
                            <l>Komm' in meinen Rosengarten,</l>
                            <l>ich will mit dir geh'n,</l>
                            <l>all die Blüten schön und zarten</l>
                            <l>werden uns umweh'n!</l>
                            <l>Warte bis der Abendglokken</l>
                            <l>friedlich Lied verklingt,</l>
                            <l>und die Amsel im Frohlokken</l>
                            <l>ihre Lieder singt.</l>
                        </lg>
                        <lg n="1" type="chorous">
                            <l>Komm' in meinen Rosengarten,</l>
                            <l>komm', komm', komm!</l>
                        </lg>
                        <lg n="2" type="stanza">
                            <l>Dann im süßen Duft der Rosen</l>
                            <l>gibt sich Zauber kund</l>
                            <l>und der Liebe Engel kosen</l>
                            <l>zärtlich uns den Mund</l>
                            <l>Heiss wird unser Herz umfangen</l>
                            <l>eine frohe Zeit,</l>
                            <l>und des Duftes süss Verlangen</l>
                            <l>wird zur Heimlichkeit.</l>
                        </lg>
                        <lg n="2" type="chorous">
                            <l>Komm' in meinen Rosengarten,</l>
                            <l>komm', komm', komm!</l>
                        </lg>
                    </div>
                </score>
            </mdiv>
        </body>
    </music>
