    <music>
        <facsimile/>
        <body>
            <mdiv xml:id="baudi_20_926230e0" label="Fern der Heimat">
                <score>
                    <scoreDef/>
                    <section/>
                    <div type="songtext">
                        <lg label="title">
                            <l label="title"/>
                        </lg>
                        <lg n="1" xml:id="baudi-01-a3dfc3f9-stanza-0001">
                            <l n="1" xml:id="baudi-01-a3dfc3f9-verse-0001">Wenn in schönen Frühlingstagen</l>
                            <l n="2" xml:id="baudi-01-a3dfc3f9-verse-0002">Sonne in mein Stübchen lacht,</l>
                            <l n="3" xml:id="baudi-01-a3dfc3f9-verse-0003">und die Bienen Honig tragen,</l>
                            <l n="4" xml:id="baudi-01-a3dfc3f9-verse-0004">Wanderlust in mir erwacht</l>
                            <l n="5" xml:id="baudi-01-a3dfc3f9-verse-0005">Dann nehm' ich mir meinen Stecken,</l>
                            <l n="6" xml:id="baudi-01-a3dfc3f9-verse-0006">meinen Mantel und den Hut,</l>
                            <l n="7" xml:id="baudi-01-a3dfc3f9-verse-0007">such' noch in verborgnen Ecken</l>
                            <l n="8" xml:id="baudi-01-a3dfc3f9-verse-0008">meiner Schuhe bestes Gut.</l>
                            <l n="9" xml:id="baudi-01-a3dfc3f9-verse-0009">Tralala, tralala!</l>
                            <l n="10" xml:id="baudi-01-a3dfc3f9-verse-0010">meiner Schuhe bestes Gut.</l>
                        </lg>
                        <lg n="2" xml:id="baudi-01-a3dfc3f9-stanza-0002">
                            <l n="11" xml:id="baudi-01-a3dfc3f9-verse-0011">Stille Freude hält umfangen</l>
                            <l n="12" xml:id="baudi-01-a3dfc3f9-verse-0012">schon mein Herz für diesen Tag,</l>
                            <l n="13" xml:id="baudi-01-a3dfc3f9-verse-0013">und der Sehnsucht heiss Verlangen</l>
                            <l n="14" xml:id="baudi-01-a3dfc3f9-verse-0014">ich in mir erschauernd trag!</l>
                            <l n="15" xml:id="baudi-01-a3dfc3f9-verse-0015">Schreit' ich froh durch Wald und Fluren,</l>
                            <l n="16" xml:id="baudi-01-a3dfc3f9-verse-0016">spür ich Gottes reiche Macht,</l>
                            <l n="17" xml:id="baudi-01-a3dfc3f9-verse-0017">dann ist's mir, als ging ich Spuren</l>
                            <l n="18" xml:id="baudi-01-a3dfc3f9-verse-0018">wo die Sonne herge[-]lacht</l>
                            <l n="19" xml:id="baudi-01-a3dfc3f9-verse-0019">Tralala, tralala!</l>
                            <l n="20" xml:id="baudi-01-a3dfc3f9-verse-0020">wo die Sonne hergelacht</l>
                        </lg>
                    </div>
                </score>
            </mdiv>
